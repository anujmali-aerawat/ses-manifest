{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ProtectCfCTExecutionRoles",
      "Effect": "Deny",
      "Action": [
        "iam:DeleteRole",
        "iam:AttachRolePolicy",
        "iam:DetachRolePolicy",
        "iam:DeleteRolePolicy",
        "iam:UpdateAssumeRolePolicy"
      ],
      "Resource": [
        "arn:aws:iam::*:role/AWSControlTowerExecution",
        "arn:aws:iam::*:role/stacksets-exec-*"
      ]
    },
    {
      "Sid": "ProtectSaaSDeploymentBucket",
      "Effect": "Deny",
      "Action": [
        "s3:DeleteBucket",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::*-customizations-*"
    }
  ]
}